{
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "productName": "NiLLM",
  "version": "1.0.3",
  "identifier": "com.nillm.app",
  "plugins": {
    "process": {
      "active": true
    }
  },
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "title": "NiLLM",
        "width": 1000,
        "height": 600,
        "dragDropEnabled": false
      }
    ]
  }
}
